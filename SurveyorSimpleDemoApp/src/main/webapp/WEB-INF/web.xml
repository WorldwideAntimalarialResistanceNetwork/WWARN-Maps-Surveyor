<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">
    <display-name>SurveyorSimpleDemoApp</display-name>
    <servlet>
        <servlet-name>org.wwarn.surveyor.SurveyorCore SearchService</servlet-name>
        <servlet-class>org.wwarn.surveyor.server.core.SearchServiceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>org.wwarn.surveyor.SurveyorCore SearchService</servlet-name>
        <url-pattern>/Project/SearchService</url-pattern>
    </servlet-mapping>
</web-app>
